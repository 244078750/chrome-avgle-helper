{
	"env": {
		"browser": true,
		"commonjs": true,
		"es6": true,
		"node": true
	},
	"globals": {
		"chrome": true,
		"videojs": true
	},
	"parser": "babel-eslint",
	"parserOptions": {
		"sourceType": "module"
	},
	"extends": "eslint:recommended",
	"rules": {
		"no-unused-vars": [
			"warn",
			{
				"varsIgnorePattern": "(?:noop)",
				"args": "none"
			}
		],
		"no-console": "off"
	}
}
